{
  "engines": {
    "node": "^12.0.0",
    "npm": "^6.0.0"
  },
  "name": "read-package-engines-version-actions",
  "version": "1.1.0",
  "private": true,
  "description": "Output node and npm version numbers from package.json",
  "main": "lib/main.js",
  "scripts": {
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint --fix src/**/*.ts",
    "precommit": "npm run build && lint-staged && npm run test",
    "build": "tsc",
    "build:watch": "tsc -w",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/skjnldsv/read-package-engines-version-actions.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "skjnldsv",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.6.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.13",
    "@types/node": "^12.0.4",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "@vercel/ncc": "^0.28.6",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.6.0",
    "eslint-plugin-prettier": "^3.1.1",
    "jest": "^24.8.0",
    "jest-circus": "^29.3.1",
    "lint-staged": "^9.4.3",
    "prettier": "^2.5.1",
    "ts-jest": "^24.3.0",
    "typescript": "^3.5.1"
  }
