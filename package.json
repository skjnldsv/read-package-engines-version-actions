{
  "name": "@skjnldsv/read-package-engines-version-actions",
  "description": "Output node and npm version numbers from package.json",
  "private": true,
  "main": "lib/main.js",
  "scripts": {
    "lint": "eslint src/*.ts src/**/*.ts",
    "lint:fix": "eslint --fix src/*.ts src/**/*.ts",
    "build": "tsc",
    "build:watch": "tsc -w",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest",
    "prettier": "prettier --check src/*.ts src/**/*.ts",
    "prettier:fix": "prettier --write src/*.ts src/**/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/skjnldsv/read-package-engines-version-actions.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "skjnldsv",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.2",
    "@types/node": "^20.11.15",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "@vercel/ncc": "^0.38.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.5.0",
    "jest-circus": "^29.7.0",
    "lint-staged": "^13.2.2",
    "prettier": "^3.2.4",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": "^20.0.0",
    "npm": "^10.0.0"
  }
}
